{
    "manifest_version": 3,
    "name": "Stress Control",
    "version": "1.0",
    "description": "Manage persistent stress with Web Workers in a detached window.",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": "images/icon.png"
    }
    
  }
  